: BrunelJQueryControlFactory                    }                  }              }           }      });      require([\"d3\"], function(d3) {         require([\"brunel\", \"brunelControls\"], function(brunel, brunelControls) {             function  BrunelVis(visId) {\r\n",
       "  \"use strict\"; // Strict Mode\r\n",
